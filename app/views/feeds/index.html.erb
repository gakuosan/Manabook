  <tbody>
    <div class="row">
    <% @feeds.each do |feed| %>
    
    <div class="col-xs-12 col-md-6 blog-panel">
      <tr>
        <td><%= feed.image %></td>
         <div class="blog-links">
          <%= link_to 'Show', feed, class: 'btn btn-default' %>
